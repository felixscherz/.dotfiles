{
    "python.formatting.provider": "yapf",
    "python.formatting.yapfArgs": [
        "--style",
        "google"
    ],
    "python.linting.flake8Enabled": true,
    "Lua.diagnostics.globals": [
        "vim",
        "use"
    ],
    "yaml.customTags": [
        "!Equals sequence",
        "!FindInMap sequence",
        "!GetAtt",
        "!GetAZs",
        "!ImportValue",
        "!Join sequence",
        "!Ref",
        "!Select sequence",
        "!Split sequence",
        "!Sub"
    ]
}
